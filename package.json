{
  "name": "lotusscript",
  "displayName": "Lotus Script",
  "description": "LotusScript highlighting support for vscode",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.x.0"
  },
  "publisher": "timchen-ixq",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "lotusscript",
      "aliases": ["LotusScript", "lotusscript"],
      "extensions": [".lss"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "lotusscript",
      "scopeName": "source.lss",
      "path": "./syntaxes/lotusscript.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "lotusscript",
        "path": "./snippets/lotusscript.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/InfernoPC/vscode-lotusscript"
  }
}
